@model List<DetailsEmployeeViewModel>

@{
    ViewData["Title"] = "Our Team";
}

<div class="container col-sm-12 offset-lg-2 col-lg-6 offset-xl-3 col-xl-6">
    <h2 class="text-center">Employees</h2>
    <table class="table">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Age</th>
                <th>Position</th>
                <th>Hire Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.FirstName</td>
                    <td>@employee.LasttName</td>
                    <td>@employee.Age</td>
                    <td>@employee.Position</td>
                    <td>@employee.HireDate</td>
                </tr>
            }
        </tbody>
    </table>
</div>